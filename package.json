{
  "repository": {
    "type": "git",
    "url": "https://github.com/rodclemen/color-inspector.git"
  },
  "homepage": "https://github.com/rodclemen/color-inspector",
  "bugs": {
    "url": "https://github.com/rodclemen/color-inspector/issues"
  },
  "description": "A focused color usage inspector for VS Code. Follows your imports. Shows where colors actually live. No magic. No guessing.",
  "license": "MIT",
  "name": "color-inspector",
  "displayName": "color-inspector",
  "version": "0.0.2",
  "publisher": "rodclemen",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Color Inspector",
      "properties": {
        "colorInspector.autoScanMinutes": {
          "type": "number",
          "default": 0,
          "minimum": 0,
          "maximum": 10,
          "description": "Auto-scan interval in minutes. 0 = Off. Range: 1â€“10."
        }
      }
    },
    "commands": [
      {
        "command": "color-inspector.scan",
        "title": "Color Inspector: Refresh"
      },
      {
        "command": "color-inspector.openSettings",
        "title": "Color Inspector: Open Settings"
}
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "colorInspector",
          "title": "Color Inspector",
          "icon": "media/palette.svg"
        }
      ]
    },
    "views": {

      

      "colorInspector": [
        {
          "icon": "media/palette.svg",
          "id": "colorInspector.view",
          "name": "Color Inspector",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.19.0",
    "typescript": "^5.9.3"
  }
}
